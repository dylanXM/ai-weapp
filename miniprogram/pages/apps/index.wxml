<t-navbar left-arrow="{{ false }}" title="应用列表" style="--td-navbar-bg-color: #f1f1f1;" />

<view
  style="height: calc(100vh - {{navBar.navBarHeight}}px)"
  class="p-apps-container"
>
  <view>
    <t-cell
      leftIcon="letters-x"
      size="large"
      title="思维导图"
      is-link
      bind:tap="clickMindMap"
    />
  </view>

  <view class="p-apps-item">
    <t-cell
      leftIcon="image"
      size="large"
      title="DELL画图"
      is-link
      bind:tap="clickDelle"
    />
    <t-cell
    leftIcon="image-1"
      size="large"
      title="midjounery绘图"
      is-link
      bind:tap="clickMJ"
    />
  </view>

  <view class="p-apps-item">
    <t-cell
      leftIcon="user-talk"
      size="large"
      title="语音交互"
      is-link
      bind:tap="clicVoice"
    />
  </view>
</view>